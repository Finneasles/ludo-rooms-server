{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "main": "build/agent.js",
  "scripts": {
    "dev": "ts-node-dev  --prefer-ts true --no-notify -r tsconfig-paths/register --watch src --respawn --transpile-only src/agent.ts",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && cp package*.json build && cd build && npm ci --omit=dev && rm package*.json",
    "start": "node build/agent.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.3",
    "socket.io": "^4.5.4",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  }
}
